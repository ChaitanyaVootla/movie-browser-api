{
    "name": "movie-browser-api",
    "version": "1.0.0",
    "description": "API for enriching TMDB data with additional movie information",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "scrape:test": "npm run build && node dist/services/scraping/ratingsScraper.js",
        "scrape:test:local": "ts-node src/scripts/testScraper.ts",
        "scrape:test:google": "ts-node src/scripts/testGoogleScraper.ts",
        "deploy:scrapers": "npm run build && sls deploy --verbose",
        "deploy:scrapers:dev": "npm run build && sls deploy --stage dev --verbose",
        "deploy:scrapers:prod": "npm run build && sls deploy --stage prod --verbose",
        "update:api-config": "ts-node src/scripts/updateApiConfig.ts",
        "test": "jest",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write \"src/**/*.{ts,js,json,md}\" \"*.{js,json,md}\"",
        "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\" \"*.{js,json,md}\"",
        "migrate": "knex migrate:latest",
        "migrate:rollback": "knex migrate:rollback",
        "migrate:make": "knex migrate:make",
        "seed": "knex seed:run",
        "seed:make": "knex seed:make",
        "populate:movies": "ts-node src/scripts/populateMovies.ts",
        "populate:movies:dev": "ts-node-dev --transpile-only src/scripts/populateMovies.ts",
        "check:movie": "ts-node src/scripts/checkMovieRelations.ts"
    },
    "keywords": [
        "movies",
        "api",
        "tmdb",
        "ratings"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/async": "^3.2.24",
        "@types/knex": "^0.16.1",
        "async": "^3.2.6",
        "axios": "^1.8.4",
        "cheerio": "^1.0.0",
        "chrome-aws-lambda": "^10.1.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.18.2",
        "helmet": "^6.0.1",
        "knex": "^2.5.1",
        "node-fetch": "^2.7.0",
        "pg": "^8.9.0",
        "puppeteer-core": "^10.1.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.148",
        "@types/cheerio": "^0.22.35",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/node": "^18.15.11",
        "@types/node-fetch": "^2.6.12",
        "@types/pg": "^8.6.6",
        "prettier": "^3.5.3",
        "serverless": "^4.12.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.3"
    }
}
